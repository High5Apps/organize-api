          <hashTree/>
          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="encryptedOrgName, encryptedOrgMemberDefinition" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import groovy.json.JsonBuilder
import io.jsonwebtoken.io.Decoders
import io.jsonwebtoken.io.Encoders
import java.nio.charset.StandardCharsets;
import java.security.SecureRandom
import javax.crypto.Cipher
import javax.crypto.spec.SecretKeySpec

class EncryptedMessage {
    String c
    String n
    String t
}

def encrypt(String message, byte[] symmetricKey) {
	def secretKey = new SecretKeySpec(symmetricKey, &apos;AES&apos;)
	def cipher = Cipher.getInstance(&apos;AES/GCM/NoPadding&apos;)
	cipher.init(Cipher.ENCRYPT_MODE, secretKey)
	
	def messageBytes = message.getBytes(StandardCharsets.UTF_8)
	def ciphertextAndIntegrityCheck = cipher.doFinal(messageBytes)
	
	def totalLength = ciphertextAndIntegrityCheck.length
	def ciphertextLength = totalLength - 16
	def ciphertext = Encoders.BASE64.encode(Arrays.copyOfRange(ciphertextAndIntegrityCheck, 0, ciphertextLength))
	def integrityCheck = Encoders.BASE64.encode(Arrays.copyOfRange(ciphertextAndIntegrityCheck, ciphertextLength, totalLength))
	def initializationVector = Encoders.BASE64.encode(cipher.getIV())

	def encryptedMessage = new EncryptedMessage(c: ciphertext, n: initializationVector, t: integrityCheck)
	return new JsonBuilder(encryptedMessage).toString()
}

def groupSecretBase64 = props.get(&quot;groupSecretBase64&quot;)
def groupSecret = Decoders.BASE64.decode(groupSecretBase64)

def encryptedOrgName = encrypt(&apos;Local 1234&apos;, groupSecret)
def encryptedOrgMemberDefinition = encrypt(&apos;An employee of Global Co. at store #9876&apos;, groupSecret)

vars.putObject(&quot;encryptedOrgName&quot;, encryptedOrgName)
vars.putObject(&quot;encryptedOrgMemberDefinition&quot;, encryptedOrgMemberDefinition)</stringProp>
