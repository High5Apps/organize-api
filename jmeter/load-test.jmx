def publicKeyPem = &quot;-----BEGIN PUBLIC KEY-----\\\\n${encodedPublicKeyBase64}\\\\n-----END PUBLIC KEY-----&quot;
vars.put(&quot;publicKeyPem&quot;, publicKeyPem);</stringProp>
