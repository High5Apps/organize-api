          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="groupSecretBase64 prop" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
